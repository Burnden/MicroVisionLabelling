   "source": [
    "from multiprocessing import Pool\n",
    "import time\n",
    "\n",
    "def f(x):\n",
    "    return x*x\n",
    "\n",
    "pool = Pool(processes=4)              # start 4 worker processes\n",
    "\n",
    "result = pool.apply_async(f, (10,))   # evaluate \"f(10)\" asynchronously in a single process\n"
   ]
  },
